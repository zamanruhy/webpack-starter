{
  "name": "webpack-starter",
  "version": "1.0.0",
  "description": "Webpack starter",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --progress --config webpack/webpack.config.dev.js",
    "build": "cross-env NODE_ENV=production node webpack/prod.js",
    "build:analyze": "cross-env BUNDLE_ANALYZE=true npm run build",
    "build:server": "npm run build && http-server dist -g -c2629746",
    "lint": "eslint --ext .js,.vue src tests",
    "lint:fix": "eslint --ext .js,.vue src tests --fix",
    "test": "cross-env NODE_ENV=test jest",
    "add-component": "babel-node --presets @babel/preset-env add-component.js"
  },
  "keywords": [
    "vue",
    "scss",
    "svg",
    "hbs"
  ],
  "author": "Ruslan Shakirov",
  "license": "MIT",
  "dependencies": {
    "@linusborg/vue-simple-portal": "^0.1.4",
    "core-js": "^3.6.2",
    "sanitize.css": "^11.0.0",
    "swiper": "^5.2.1",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-syntax-jsx": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/register": "^7.7.7",
    "@babel/runtime": "^7.7.7",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "@vue/babel-preset-jsx": "^1.1.2",
    "@vue/test-utils": "^1.0.0-beta.30",
    "autoprefixer": "^9.7.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "cache-loader": "^4.1.0",
    "chalk": "^3.0.0",
    "compression-webpack-plugin": "^3.0.1",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^5.2.3",
    "express": "^4.17.1",
